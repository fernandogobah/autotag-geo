{
  "name": "autotag-geo",
  "version": "1.0.0",
  "description": "Plugin para automatização de tags de geolocalização (cidade, estado, país, provedor)",
  "author": "Go!Bot",
  "main": "main.js",
  "enabled": true,
  "permissions": [
    "mesh:read",
    "mesh:write",
    "tag:write"
  ],
  "settings": {
    "updateInterval": 3600000,
    "geoApiUrl": "http://ip-api.com/json/",
    "tagPrefix": {
      "city": "Cidade:",
      "state": "Estado:",
      "country": "Pais:",
      "provider": "Provedor:"
    }
  }
}
